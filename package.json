{
  "name": "ng-harmony-annotate",
  "version": "0.1.1",
  "description": "Angular 2.0 like annotations",
  "main": "./dist/es5/system_module.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "literate-programming README.md",
    "beautify": "sed -i 's/^\\s*//' ./dist/raw/ng_harmony_annotate.js && js-beautify -m 1 -s 4 -l 0 -p ./dist/raw/ng_harmony_annotate.js > ./dist/esnext/ng_harmony_annotate.js",
    "transpile_amd": "babel --modules amd -e 1 -s -o ./dist/es5/amd_module.js ./dist/esnext/ng_harmony_annotate.js",
    "transpile_common": "babel --modules common -e 1 -s -o ./dist/es5/common_module.js ./dist/esnext/ng_harmony_annotate.js",
    "transpile_system": "babel --modules system -e 1 -s -o ./dist/es5/system_module.js ./dist/esnext/ng_harmony_annotate.js",
    "transpile_umd": "babel --modules umd -e 1 -s -o ./dist/es5/umd_module.js ./dist/esnext/ng_harmony_annotate.js",
    "all": "npm run build && npm run beautify && npm run transpile_amd && npm run transpile_common && npm run transpile_system && npm run transpile_umd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ng-harmony/ng-harmony-annotate.git"
  },
  "keywords": [
    "angular",
    "angular",
    "2.0",
    "annotate",
    "annotation",
    "At-Script",
    "Typescript",
    "es-next",
    "harmony"
  ],
  "author": "Joehannes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ng-harmony/ng-harmony-annotate/issues"
  },
  "homepage": "https://github.com/ng-harmony/ng-harmony-annotate",
  "devDependencies": {
    "babel": "^5.1.10",
    "js-beautify": "^1.5.5",
    "literate-programming": "^0.8.4"
  },
  "jspm": {
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
